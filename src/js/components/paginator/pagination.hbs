<div class="col-md-4 col-lg-3"></div>
<nav aria-label="Page navigation" class="col-md-8 col-lg-9 shadow-lg bg-light m-0">
    <ul class="pagination pg-blue justify-content-center align-items-center my-1">
        <li class="page-item {{#if (eq currentPage 0)}}disabled{{/if}}">
            <a id="page-changer" data-page="0" class="page-link">
                <svg viewBox="0 0 16 16" class="bi bi-chevron-bar-left" fill="currentColor">
                    <path fill-rule="evenodd"
                        d="M11.854 3.646a.5.5 0 0 1 0 .708L8.207 8l3.647 3.646a.5.5 0 0 1-.708.708l-4-4a.5.5 0 0 1 0-.708l4-4a.5.5 0 0 1 .708 0zM4.5 1a.5.5 0 0 0-.5.5v13a.5.5 0 0 0 1 0v-13a.5.5 0 0 0-.5-.5z" />
                </svg>
            </a>
        </li>
        <li class="page-item {{#if (eq currentPage 0)}}disabled{{/if}}">
            <a id="page-changer" data-page="{{sub currentPage 1}}" class="page-link" aria-label="Previous">
                <span aria-hidden="true">
                    <svg viewBox="0 0 16 16" class="bi bi-chevron-double-left" fill="currentColor">
                        <path fill-rule="evenodd"
                            d="M8.354 1.646a.5.5 0 0 1 0 .708L2.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0z" />
                        <path fill-rule="evenodd"
                            d="M12.354 1.646a.5.5 0 0 1 0 .708L6.707 8l5.647 5.646a.5.5 0 0 1-.708.708l-6-6a.5.5 0 0 1 0-.708l6-6a.5.5 0 0 1 .708 0z" />
                    </svg>
                </span>
                <span class="sr-only">Poprzednia</span>
            </a>
        </li>
        {{set "begin" (limitMin (sub currentPage 2) 0)}}
        {{set "end" (limitMax (add currentPage 2) totalPages)}}
        {{#loop "begin" "end"}}
            <li class="page-item {{#if (eq @index currentPage)}}active{{/if}}">
                <a id="page-changer" data-page="{{@index}}" class="page-link">{{add @index 1}}</a>
            </li>
        {{/loop}}
        <li class="page-item {{#if (eq currentPage totalPages)}}disabled{{/if}}">
            <a id="page-changer" data-page="{{add currentPage 1}}" class="page-link" aria-label="Next">
                <span aria-hidden="true">
                    <svg viewBox="0 0 16 16" class="bi bi-chevron-double-right" fill="currentColor">
                        <path fill-rule="evenodd"
                            d="M3.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L9.293 8 3.646 2.354a.5.5 0 0 1 0-.708z" />
                        <path fill-rule="evenodd"
                            d="M7.646 1.646a.5.5 0 0 1 .708 0l6 6a.5.5 0 0 1 0 .708l-6 6a.5.5 0 0 1-.708-.708L13.293 8 7.646 2.354a.5.5 0 0 1 0-.708z" />
                    </svg>
                </span>
                <span class="sr-only">Następna</span>
            </a>
        </li>
        <li class="page-item {{#if (eq currentPage totalPages)}}disabled{{/if}}">
            <a id="page-changer" data-page="{{totalPages}}" class="page-link">
                <svg viewBox="0 0 16 16" class="bi bi-chevron-bar-right" fill="currentColor">
                    <path fill-rule="evenodd"
                        d="M4.146 3.646a.5.5 0 0 0 0 .708L7.793 8l-3.647 3.646a.5.5 0 0 0 .708.708l4-4a.5.5 0 0 0 0-.708l-4-4a.5.5 0 0 0-.708 0zM11.5 1a.5.5 0 0 1 .5.5v13a.5.5 0 0 1-1 0v-13a.5.5 0 0 1 .5-.5z" />
                </svg>
            </a>
        </li>
        <li class="page-item">
            <div class="btn-group dropup">
                <button id="limit-changer" class="btn btn-sm btn-primary rounded-pill dropdown-toggle" type="button"
                    id="dropdownMenuButton" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                    {{#if (eq limit 0)}}
                        Wszystkie
                    {{else}}
                        {{limit}}
                    {{/if}}
                </button>
                <div class="dropdown-menu" aria-labelledby="dropdownMenuButton">
                    <a id="limit-changer" data-limit="0" class="dropdown-item" href="#">Wszystkie wyniki</a>
                    <a id="limit-changer" data-limit="5" class="dropdown-item" href="#">5 wyników</a>
                    <a id="limit-changer" data-limit="10" class="dropdown-item" href="#">10 wyników</a>
                    <a id="limit-changer" data-limit="20" class="dropdown-item" href="#">20 wyników</a>
                    <a id="limit-changer" data-limit="50" class="dropdown-item" href="#">50 wyników</a>
                    <a id="limit-changer" data-limit="100" class="dropdown-item" href="#">100 wyników</a>
                </div>
            </div>
        </li>
    </ul>
</nav>